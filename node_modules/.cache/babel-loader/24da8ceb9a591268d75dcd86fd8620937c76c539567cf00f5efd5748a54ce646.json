 reservationName: '',\n      reservationSurname: '',\n      reservationEmail: '',\n      reservationSuccess: false,\n      selectedGift: null\n    }\n  },\n  methods: {\n    addGift() {\n      this.gifts.push({\n        title: this.giftTitle,\n        price: this.giftPrice,\n        description: this.giftDescription,\n        link: this.giftLink,\n        image: this.giftImage\n      })\n      this.giftTitle = ''\n      this.giftPrice = ''\n      this.giftDescription = ''\n      this.giftLink = ''\n      this.giftImage = ''\n    },\n    openReservationForm(gift) {\n      this.showReservationForm = true\n      this.selectedGift = gift\n    },\n    submitReservation() {\n      // send reservation details to server or store in local storage\n      this.gifts.splice(this.gifts.indexOf(this.selectedGift), 1)\n      this.showReservationForm = false\n      this.reservationSuccess = true\n    }\n  }\n}\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}