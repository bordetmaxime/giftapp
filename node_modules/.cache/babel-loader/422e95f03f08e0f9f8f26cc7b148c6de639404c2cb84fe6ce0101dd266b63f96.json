{"ast":null,"code":"export default {\n  name: 'ListPage',\n  data() {\n    return {\n      firstnameSearch: this.$route.params.firstnameSearch,\n      lastnameSearch: this.$route.params.lastnameSearch,\n      gifts: [{\n        id: 1,\n        title: \"tomate\",\n        price: \"15e\",\n        description: \"tomate de quali\",\n        link: \"tomate@tomate\",\n        image: \"tomate.jpegege\"\n      }, {\n        id: 2,\n        title: \"carotte\",\n        price: \"25e\",\n        description: \"dsd\",\n        link: \"tomate@tomate\",\n        image: \"tomate.jpegege\"\n      }],\n      showReservationForm: false,\n      reservationName: '',\n      reservationSurname: '',\n      reservationEmail: '',\n      reservationSuccess: false,\n      selectedGift: null\n    };\n  },\n  methods: {\n    openReservationForm(gift) {\n      this.showReservationForm = true;\n      this.selectedGift = gift;\n    },\n    submitReservation() {\n      // send reservation details to server or store in local storage\n      this.gifts.splice(this.gifts.indexOf(this.selectedGift), 1);\n      this.showReservationForm = false;\n      this.reservationSuccess = true;\n    }\n  }\n};","map":{"version":3,"mappings":"AAyCA,eAAe;EAEPA,IAAI,EAAE,UAAU;EAChBC,IAAI,GAAG;IACX,OAAO;MACLC,eAAe,EAAE,IAAI,CAACC,MAAM,CAACC,MAAM,CAACF,eAAe;MACnDG,cAAc,EAAE,IAAI,CAACF,MAAM,CAACC,MAAM,CAACC,cAAc;MACjDC,KAAK,EAAE,CAAC;QACNC,EAAE,EAAC,CAAC;QACJC,KAAK,EAAE,QAAQ;QACfC,KAAK,EAAE,KAAK;QACZC,WAAW,EAAE,iBAAiB;QAC9BC,IAAI,EAAE,eAAe;QACrBC,KAAK,EAAE;MACT,CAAC,EACN;QAAEL,EAAE,EAAC,CAAC;QACCC,KAAK,EAAE,SAAS;QAChBC,KAAK,EAAE,KAAK;QACZC,WAAW,EAAE,KAAK;QAClBC,IAAI,EAAE,eAAe;QACrBC,KAAK,EAAE;MACT,CAAC,CAEF;MACCC,mBAAmB,EAAE,KAAK;MAC1BC,eAAe,EAAE,EAAE;MACnBC,kBAAkB,EAAE,EAAE;MACtBC,gBAAgB,EAAE,EAAE;MACpBC,kBAAkB,EAAE,KAAK;MACzBC,YAAY,EAAE;IAChB;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,mBAAmB,CAACC,IAAI,EAAE;MACxB,IAAI,CAACR,mBAAkB,GAAI,IAAG;MAC9B,IAAI,CAACK,YAAW,GAAIG,IAAG;IACzB,CAAC;IACDC,iBAAiB,GAAG;MAClB;MACA,IAAI,CAAChB,KAAK,